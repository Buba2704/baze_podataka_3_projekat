--------------------------------------------------------
--  DDL for Trigger TRG_UPDATE_PROIZVOD
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "C##KORISNIK"."TRG_UPDATE_PROIZVOD" 
AFTER UPDATE OF nazivProizvoda
ON Proizvod
FOR EACH ROW
DECLARE
PRAGMA AUTONOMOUS_TRANSACTION;
BEGIN
EXECUTE IMMEDIATE 'ALTER TRIGGER TRG_UPDATE_NAZIVPROIZVODA DISABLE';
EXECUTE IMMEDIATE 'ALTER TRIGGER DOSTUPNAKOLICINA1 DISABLE';
EXECUTE IMMEDIATE 'ALTER TRIGGER DOSTUPNAKOLICINA2 DISABLE';
UPDATE IstorijaStanjaZaliha
SET nazivProizvoda=:NEW.nazivProizvoda
WHERE sifProizvoda =:NEW.sifProizvoda;
COMMIT;
BEGIN
EXECUTE IMMEDIATE 'ALTER TRIGGER TRG_UPDATE_NAZIVPROIZVODA ENABLE';
EXECUTE IMMEDIATE 'ALTER TRIGGER DOSTUPNAKOLICINA1 ENABLE';
EXECUTE IMMEDIATE 'ALTER TRIGGER DOSTUPNAKOLICINA2 ENABLE';
END;
END;
/
ALTER TRIGGER "C##KORISNIK"."TRG_UPDATE_PROIZVOD" ENABLE;
--------------------------------------------------------
--  DDL for Trigger PROIZVOD_AUTO_INCREMENT
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "C##KORISNIK"."PROIZVOD_AUTO_INCREMENT" 
  BEFORE INSERT ON proizvod
  FOR EACH ROW
BEGIN
  SELECT proiz_seq.nextval
  INTO :new.sifProizvoda
  FROM dual;
END;
/
ALTER TRIGGER "C##KORISNIK"."PROIZVOD_AUTO_INCREMENT" ENABLE;
