--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "C##KORISNIK"."TRG_UPDATE_DOSTUPNAKOLICINA" 
 AFTER UPDATE OF dostupnaKolicina
 ON Proizvod
 BEGIN
 IF isz.proiz = 0 THEN
 RAISE_APPLICATION_ERROR (
 NUM => -20000,
 MSG => 'NE SMETE DA MENJATE POLJE DOSTUPNAKOLICINA IZ TABELE PROIZVOD, JER SE ONO SAMO OBRACUNAVA');
 END IF;
 END;
