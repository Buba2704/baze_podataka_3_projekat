--------------------------------------------------------
--  DDL for Trigger TRG_INSERT_ISTORIJA
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "C##KORISNIK"."TRG_INSERT_ISTORIJA" 
 BEFORE INSERT
 ON IstorijaStanjaZaliha
 FOR EACH ROW
 BEGIN
 SELECT nazivProizvoda INTO :NEW.nazivProizvoda
 FROM Proizvod
 WHERE sifProizvoda =:NEW.sifProizvoda;
END;
/
ALTER TRIGGER "C##KORISNIK"."TRG_INSERT_ISTORIJA" ENABLE;
--------------------------------------------------------
--  DDL for Trigger DOSTUPNAKOLICINA1
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "C##KORISNIK"."DOSTUPNAKOLICINA1" 
BEFORE INSERT OR UPDATE OR DELETE ON IstorijaStanjaZaliha
FOR EACH ROW
BEGIN
IF (INSERTING OR UPDATING)
THEN
BEGIN isz.proiz:=:NEW.sifProizvoda;END;
ELSE
BEGIN isz.proiz:=:OLD.sifProizvoda;END;
END IF;
END;
/
ALTER TRIGGER "C##KORISNIK"."DOSTUPNAKOLICINA1" ENABLE;
--------------------------------------------------------
--  DDL for Trigger TRG_UPDATE_POSLDATUMTREB
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "C##KORISNIK"."TRG_UPDATE_POSLDATUMTREB" 
 AFTER UPDATE OF poslDatumTreb
 ON Magacin
 BEGIN
 IF treb.magacin = 0 THEN
 RAISE_APPLICATION_ERROR (
 NUM => -20000,
 MSG => 'NE SMETE DA MENJATE POLJE POSLDATUMTREB IZ TABELE MAGACIN, JER SE ONO SAMO OBRACUNAVA');
 END IF;
 END;
/
ALTER TRIGGER "C##KORISNIK"."TRG_UPDATE_POSLDATUMTREB" ENABLE;
--------------------------------------------------------
--  DDL for Trigger OTPAD_INSERT_TRIGGER
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "C##KORISNIK"."OTPAD_INSERT_TRIGGER" 
before insert or update of nazivOtpada
on otpad
for each row
begin
select nazivVrsteOtpada into :new.nazivVrsteOtpada
from vrstaotpada
where vrstaOtpadaID = :new.vrstaOtpadaID;
end;
/
ALTER TRIGGER "C##KORISNIK"."OTPAD_INSERT_TRIGGER" ENABLE;
--------------------------------------------------------
--  DDL for Trigger TRG_UPDATE_PROIZVOD_JM
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "C##KORISNIK"."TRG_UPDATE_PROIZVOD_JM" 
AFTER UPDATE OF sifJM
ON Proizvod
FOR EACH ROW
DECLARE
PRAGMA AUTONOMOUS_TRANSACTION;
BEGIN
EXECUTE IMMEDIATE 'ALTER TRIGGER TRG_UPDATE_SIFJM_STAVKAZOPM DISABLE';
UPDATE StavkaZapisnikaOPrijemuMaterijala
SET sifJM = :NEW.sifJM
WHERE sifProizvoda=:NEW.sifProizvoda;
COMMIT;
BEGIN
EXECUTE IMMEDIATE 'ALTER TRIGGER TRG_UPDATE_SIFJM_STAVKAZOPM ENABLE';
END; 
END;
/
ALTER TRIGGER "C##KORISNIK"."TRG_UPDATE_PROIZVOD_JM" ENABLE;
