--------------------------------------------------------
--  DDL for Type OBJ_NAZIV_PAK
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TYPE "C##KORISNIK"."OBJ_NAZIV_PAK" AS OBJECT(
tip VARCHAR2(10),
tezina NUMBER(5,3),
MEMBER FUNCTION get_tip RETURN VARCHAR2,
MEMBER FUNCTION get_tezina_u_t RETURN NUMBER,
MEMBER FUNCTION get_tezina_u_kg RETURN NUMBER)
INSTANTIABLE NOT FINAL;
/
CREATE OR REPLACE EDITIONABLE TYPE BODY "C##KORISNIK"."OBJ_NAZIV_PAK" AS
MEMBER FUNCTION get_tip RETURN varchar2 IS
BEGIN
RETURN SELF.tip;
END;
MEMBER FUNCTION get_tezina_u_t RETURN number IS
BEGIN
RETURN SELF.tezina;
END;
MEMBER FUNCTION get_tezina_u_kg RETURN number IS
BEGIN
RETURN SELF.tezina * 1000;
END;
END;

/
--------------------------------------------------------
--  DDL for Type OBJ_JMBG
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TYPE "C##KORISNIK"."OBJ_JMBG" AS OBJECT
(vrednost NUMBER(13), 
MEMBER FUNCTION get_vrednost RETURN NUMBER,
MEMBER FUNCTION get_pol RETURN VARCHAR2
)
INSTANTIABLE NOT FINAL;
/
CREATE OR REPLACE EDITIONABLE TYPE BODY "C##KORISNIK"."OBJ_JMBG" AS
MEMBER FUNCTION get_vrednost RETURN number IS
BEGIN
RETURN SELF.vrednost;
END;
MEMBER FUNCTION get_pol RETURN varchar2 IS
BEGIN
IF SUBSTR(TO_CHAR(SELF.vrednost), 10, 3 ) >= 500
THEN RETURN 'Žensko';
ELSE
RETURN 'Muško';
END IF;
END;
END;

/
